{
  "name": "agenciaviajesnode",
  "version": "1.0.0",
  "description": "Primer proyecto con Node.js",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch controllers --watch models --watch views --watch public --watch routes --watch index.js index.js",
