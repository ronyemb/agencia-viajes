    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  },
  "nodemonConfig": {
    "watch": [
      "controllers",
      "models",
      "views",
      "public",
      "routes",
      "index.js"
